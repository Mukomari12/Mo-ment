{
  "expo": {
    "name": "MowmentDemo",
    "slug": "MowmentDemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding/logo_1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/branding/splash_hero.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdfbf9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/logo_1024.png",
        "backgroundColor": "#fdfbf9"
      }
    },
    "web": {
      "favicon": "./assets/branding/logo_1024.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
